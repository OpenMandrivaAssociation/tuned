---
 tuned/admin/dbus_controller.py | 2 +-
 1 file changed, 1 insertion(+), 1 deletion(-)

diff --git a/tuned/admin/dbus_controller.py b/tuned/admin/dbus_controller.py
index 3523bdd..43e9674 100644
--- a/tuned/admin/dbus_controller.py
+++ b/tuned/admin/dbus_controller.py
@@ -23,7 +23,7 @@ class DBusController(object):
 			raise TunedAdminDBusException("Cannot talk to Tuned daemon via DBus.")
 
 		try:
-			method = self._proxy.get_dbus_method(method_name)
+			method = self._proxy.get_dbus_method(method_name, dbus_interface=self._object_name)
 			return method(*args, **kwargs)
 		except dbus.exceptions.DBusException as dbus_exception:
 			raise TunedAdminDBusException("DBus call to Tuned daemon failed (%s)." % str(dbus_exception))
